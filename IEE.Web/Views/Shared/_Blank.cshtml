<body>

    @RenderBody()

    <script src="~/scripts/jquery-2.1.1.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
    <script src="~/scripts/jquery-ui.custom.min.js"></script>

    <!-- Custom and plugin javascript -->
    
    <!-- iCheck -->
    <script src="~/scripts/plugins/iCheck/icheck.min.js"></script>
    <!-- Full Calendar -->
    
    <script>
        $(document).on('ready', function () {
            $('#consultSubmit').click(function (e) {
                e.preventDefault();
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("ConsultRegister", "CmsBase")',
                    dataType: "json",
                    data: $('form').serialize(),
                    success: function (result) {
                        if (result == true) {
                            $("#Email").empty();
                            $("#Name").empty();
                            $("#School").empty();
                            $("#Class").empty();
                            $("#Phone").empty();
                            $("#Message").empty();
                            $("#return_message").removeClass("field-validation-error");
                            $("#return_message").append("Cảm ơn bạn đã gửi yêu cầu tới IEE, chúng tôi sẽ feedback tới bạn trong thời gian sớm nhất.");
                        }
                        else {
                            $("#return_message").addClass("field-validation-error");
                            $("#return_message").append("Bạn hãy kiểm tra các thông tin cần thiết trước khi gửi cho IEE");
                        }
                    },
                    error: function () {

                    }
                });
            });
        });

        function doLayout() {
            var height = $(window).height();
            $(".blank-bg").height(height);
            $(window).resize(function () {
                height = $(window).height();
                $(".blank-bg").height(height);
            });
        }

    </script>
