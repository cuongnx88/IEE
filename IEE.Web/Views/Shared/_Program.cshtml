<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Học viên anh ngữ IEE - @ViewBag.Title</title>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="keywords" content="@ViewBag.MetaKeys" />
    <meta name="description" content="@ViewBag.MetaDescription">
    <meta name="author" content="HỌC VIÊN ANH NGỮ IEE">
    <!-- Add Your favicon here -->
    <!--<link rel="icon" href="img/favicon.ico">-->
    <title>IEE</title>
    <link rel="shortcut icon" href="~/content/img/favicon.png">
    <!-- Bootstrap core CSS -->
    <link rel="stylesheet" href="~/content/css/bootstrap.min.css">
    <!-- Animation CSS -->
    <link rel="stylesheet" href="~/content/css//animate.min.css">
    <link rel="stylesheet" href="~/font-awesome/css/font-awesome.min.css">
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- Custom styles for this template -->
    <link rel="stylesheet" type="text/css" href="~/content/css/slick.css">
    <link rel="stylesheet" type="text/css" href="~/content/css/slick-theme.css">

    <link rel="stylesheet" type="text/css" href="~/content/css/sequence-theme.basic.css" media="all">
    <link rel="stylesheet" type="text/css" href="~/content/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/content/css/menu.css">

</head>
<body>
    <div class="loading"></div>
    @{Html.RenderAction("Header", "CmsBase");}

    <section id="content">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="col-md-12 program-content">
                        @RenderBody()
                    </div>
                    <div class="consultant-command">
                        <a href="#abroad-register" class="btn btn-danger oranges-section btn-rounded">Đăng ký ngay</a>
                    </div>
                </div>
            </div>
        </div>
    </section>

    @{Html.RenderAction("University", "CmsBase");}

    <section id="abroad-register" class="blank-bg">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    @{Html.RenderAction("ConsultRegister", "CmsBase");}
                </div>
            </div>
        </div>
    </section>

    @{Html.RenderAction("Footer", "CmsBase");}

    

    <script src="~/scripts/jquery-2.1.1.js"></script>
    <script src="~/scripts/bootstrap.min.js"></script>
    <script src="~/scripts/pace.min.js"></script>
    <script src="~/scripts/cbpAnimatedHeader.js"></script>
    <script src="~/scripts/classie.js"></script>
    <script src="~/scripts/wow.min.js"></script>
    <script src="~/scripts/inspinia.js"></script>
    <script src="~/scripts/slick.js"></script>
    <!-- jmslideshow plugin : extends the jmpress plugin -->
    <script src="~/scripts/jquery.jmslideshow.js"></script>
    <script src="~/scripts/modernizr.custom.48780.js"></script>
    <script src="~/scripts/skdslider.min.js"></script>

    <script src="~/scripts/imagesloaded.pkgd.min.js"></script>
    <script src="~/scripts/hammer.min.js"></script>
    <script src="~/scripts/sequence.min.js"></script>
    <script src="~/scripts/sequence-theme.basic.js"></script>
    <script src="~/scripts/move-top.js"></script>

    <script>
        $(document).on('ready', function () {
            $(".university-regular").slick({
                dots: false,
                infinite: true,
                slidesToShow: 4,
                slidesToScroll: 4,
                responsive: [
                  {
                      breakpoint: 768,
                      settings: {
                          arrows: false,
                          centerMode: true,
                          centerPadding: '0px',
                          slidesToShow: 1
                      }
                  },
                  {
                      breakpoint: 480,
                      settings: {
                          arrows: false,
                          centerMode: true,
                          centerPadding: '0px',
                          slidesToShow: 1
                      }
                  }
                ]
            });

            $('#consultSubmit').click(function (e) {
                e.preventDefault();
                $("#return_message").empty();

                $('.loading').show();
                $.ajax({
                    type: "POST",
                    url: '@Url.Action("ConsultRegister", "CmsBase")',
                    dataType: "json",
                    data: $('form').serialize(),
                    success: function (result) {
                        if (result == true)
                        {
                            $("#Email").val("");
                            $("#Name").val("");
                            $("#School").val("");
                            $("#Class").val("");
                            $("#Phone").val("");
                            $("#Message").val("");
                            $("#return_message").removeClass("field-validation-error");
                            $("#return_message").append("Cảm ơn bạn đã gửi yêu cầu tới IEE, chúng tôi sẽ feedback tới bạn trong thời gian sớm nhất.");
                            $('.loading').hide();
                        }
                        else
                        {
                            $("#return_message").addClass("field-validation-error");
                            $("#return_message").append("Bạn hãy kiểm tra các thông tin cần thiết trước khi gửi cho IEE");
                            $('.loading').hide();
                        }
                    },
                    error: function () {
                        $('.loading').hide();
                    }
                });
            });

        });
    </script>
    <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

          ga('create', 'UA-85437578-1', 'auto');
          ga('send', 'pageview');

    </script>
